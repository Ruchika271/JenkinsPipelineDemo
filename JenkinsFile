pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Building'
            }
        }
        
        stage('Build') {
            steps {
                echo 'Hello World'
            }
        }
    
  
        stage('Deploy') {
            steps {
                echo 'Deploying'
           }
        }
        
        stage('Test') {
            steps {
                echo 'Testing'
           }
        }
           
        
        stage('Release') {
            steps {
                echo 'Releasing'
           }
        }
    
        
    }
        
    
}
